{
  "name": "vue-styleguidist",
  "description": "Vue components style guide generator",
  "version": "4.43.1",
  "type": "module",
  "homepage": "https://vue-styleguidist.github.io",
  "scripts": {
    "dev": "vite",
    "compile": "tsc -p ./tsconfig.build.json",
    "compile:watch": "pnpm compile --watch",
    "test:unit": "cypress open-ct",
    "test:unit:ci": "cypress run-ct --quiet --reporter spec",
    "test:e2e": "start-server-and-test preview http://127.0.0.1:5050/ 'cypress open'",
    "test:e2e:ci": "start-server-and-test preview http://127.0.0.1:5050/ 'cypress run'",
    "typecheck": "vue-tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vue-styleguidist/vue-styleguidist.git",
    "directory": "packages/vue-styleguidist"
  },
  "bugs": {
    "url": "https://github.com/vue-styleguidist/vue-styleguidist/issues"
  },
  "license": "MIT",
  "main": "lib/scripts/index.js",
  "bin": {
    "vue-styleguidist": "lib/bin.js"
  },
  "files": [
    "lib"
  ],
  "engines": {
    "node": ">=14",
    "npm": ">=7"
  },
  "dependencies": {
    "pinia": "^2.0.6",
    "vue-router": "^4.0.12",
    "vue-docgen-api": "^4.43.1"
  },
  "devDependencies": {
    "@cypress/vite-dev-server": "^2.2.2",
    "@cypress/vue": "^3.0.5",
    "@types/micromatch": "^4.0.2",
    "@types/node": "^16.11.12",
    "@vitejs/plugin-vue": "^2.0.0",
    "@vitejs/plugin-vue-jsx": "^1.3.1",
    "cypress": "^9.1.1",
    "fast-glob": "^3.2.7",
    "micromatch": "^4.0.4",
    "start-server-and-test": "^1.14.0",
    "typescript": "4.5.4",
    "vite": "2.7.1",
    "vue": "3.2.26",
    "vue-tsc": "^0.29.8"
  },
  "peerDependencies": {
    "vite": ">= 2",
    "vue": ">= 3"
  }
}